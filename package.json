{
  "name": "archivist",
  "version": "0.0.0",
  "author": "Kyle Hoover",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1"
  },
  "scripts": {
    "build": "yarn lint && yarn build:server && yarn build:client",
    "build:client": "cd client && yarn build",
    "build:server": "yarn clean:server && tsc",
    "clean": "yarn clean:server && yarn clean:client",
    "clean:client": "cd client && rm -rf build",
    "clean:server": "rm -rf build",
    "lint": "yarn lint:server && yarn lint:client",
    "lint:client": "tslint --project ./client",
    "lint:server": "tslint --project ./",
    "start:client": "cd client && yarn start",
    "start:server": "ts-node-dev app.ts"
  }
}
