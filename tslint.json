{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark": [true, "single", "avoid-template"],
        "semicolon": [true, "never"],
        "typedef": [true, "call-signature"],
        "variable-name": {
            "options": [
                "allow-leading-underscore",
                "allow-pascal-case",
                "check-format",
                "require-const-for-all-caps"
            ]
        }
    },
    "rulesDirectory": []
}